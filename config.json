{
    "detection": {       
        "scale_factor": 1.5,
        "min_neighbors": 5,
        "skip_frames": 10,
        "output": {
            "directory": "./faces",
            "format": "png",
            "min_width": 64,
            "min_height": 64
        },
        "classifiers": [
            "./classifiers/haarcascade_frontalface_default.xml",
            "./classifiers/haarcascade_profileface.xml",
            "./classifiers/lbpcascade_profileface.xml",
            "./classifiers/lbpcascade_frontalface_improved.xml"
        ]
    }
}